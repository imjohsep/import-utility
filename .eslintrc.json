{
  "env": {
    "node": true
  },
  "rules": {
    "comma-dangle": 1,
    "eol-last": 1,
    "no-console": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-unreachable": 2,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "valid-typeof": 2
  }
}